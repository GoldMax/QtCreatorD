<?xml version="1.0"?>
<mime-info xmlns='http://www.freedesktop.org/standards/shared-mime-info'>

				<mime-type type="text/x-dsrc">
								<sub-class-of type="text/plain"/>
								<comment>D source file</comment>
								<glob pattern="*.d"/>
					</mime-type>

				<mime-type type="text/x-dint">
								<sub-class-of type="text/x-dsrc"/>
								<comment>D interface file</comment>
								<glob pattern="*.di"/>
				</mime-type>

</mime-info>
